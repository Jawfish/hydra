{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "lockFileMaintenance": {
    "automerge": true,
    "enabled": true
  },
  "packageRules": [
    {
      "automerge": true,
      "ignoreTests": false,
      "matchPackagePatterns": ["*"]
    }
  ]
}
